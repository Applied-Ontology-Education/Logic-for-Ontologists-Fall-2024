# This checks whether all the realizable entities are connected to a process through "has realization"
# Level 6

PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>
PREFIX obo: <http://purl.obolibrary.org/obo/>

SELECT ?subclass (CONCAT("Error: Realizable entity ", STR(?subclass), " doesn't have a process realizing it.") AS ?errorMessage)
WHERE {
    ?subclass rdfs:subClassOf+ obo:BFO_0000017 .

    FILTER NOT EXISTS {
        ?subclass obo:BFO_0000054 ?object .
    }
}
