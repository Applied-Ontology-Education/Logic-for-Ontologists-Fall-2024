[ rdf:type owl:Ontology
 ] .

#################################################################
#    Object Properties
#################################################################

###  https://buffalo.edu/lfocourse/assignment1p2/reflexive
dev:reflexive rdf:type owl:ObjectProperty ;
              owl:inverseOf dev:transitive ;
              rdf:type owl:ReflexiveProperty ;
              rdfs:label "reflexive"@en .


###  https://buffalo.edu/lfocourse/assignment1p2/transitive
dev:transitive rdf:type owl:ObjectProperty ,
                        owl:TransitiveProperty ;
               rdfs:label "transitive"@en .


#################################################################
#    Classes
#################################################################

###  https://buffalo.edu/lfocourse/assignment1p2/TestThing
dev:TestThing rdf:type owl:Class ;
              rdfs:label "Test Thing"@en .


#################################################################
#    Individuals
#################################################################

###  https://buffalo.edu/lfocourse/assignment1p2/x
dev:x rdf:type owl:NamedIndividual ,
               dev:TestThing ;
      dev:transitive dev:y ;
      rdfs:label "x"@en .


###  https://buffalo.edu/lfocourse/assignment1p2/y
dev:y rdf:type owl:NamedIndividual ,
               dev:TestThing ;
      dev:transitive dev:z ;
      rdfs:label "y"@en .


###  https://buffalo.edu/lfocourse/assignment1p2/z
dev:z rdf:type owl:NamedIndividual ,
               dev:TestThing ;
      rdfs:label "z"@en .


#################################################################
#    General axioms
#################################################################

[ rdf:type owl:AllDifferent ;
  owl:distinctMembers ( dev:x
                        dev:y
                      )
] .


[ rdf:type owl:AllDifferent ;
  owl:distinctMembers ( dev:x
                        dev:z
                      )
] .


[ rdf:type owl:AllDifferent ;
  owl:distinctMembers ( dev:y
                        dev:z
                      )
] .


